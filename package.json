{
  "name": "@domtwlee/redux-api-actions",
  "version": "2.1.0",
  "description": "Redux middleware for dispatching api actions",
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "scripts": {
    "commit": "npx git-cz",
    "build": "microbundle  -f es,cjs",
    "clean": "rimraf dist/",
    "test": "jest",
    "lint": "eslint 'src/**/*.js'",
    "lint:fix": "eslint --fix 'src/**/*.js'",
    "pretty": "prettier --write 'src/**/*.js'",
    "format": "npm run lint:fix && npm run pretty"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dominictwlee/redux-api-actions.git"
  },
  "files": [
    "dist",
    "index.d.ts"
  ],
  "keywords": [
    "redux",
    "middleware",
    "flux",
    "api",
    "redux-middleware"
  ],
  "author": "Dom Lee",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dominictwlee/redux-api-actions/issues"
  },
  "homepage": "https://github.com/dominictwlee/redux-api-actions#readme",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.13.0",
    "eslint-config-domtwlee": "^1.1.2",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^1.3.1",
    "jest": "^24.1.0",
    "lint-staged": "^9.2.5",
    "microbundle": "^0.12.0-next.6",
    "prettier": "^1.16.4",
    "rimraf": "^2.6.3"
  }
}
